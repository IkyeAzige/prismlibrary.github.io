/* CSS por Rian Design - www.riandesign.com.br */








@media screen {
	
	form input[type=text],
	form input[type=email],
	select,
	textarea {
		background: #fff;
		border: none;
		font-weight: 300;
		color: #555;
		padding: 12px 12px;
		font-size: 13px;
		font-family: "Raleway", Arial, Helvetica, sans-serif;
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.1);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.1);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.1);
	}
	form input[type=text]:focus,
	form input[type=email]:focus,
	select:focus,
	textarea:focus {
		-webkit-box-shadow: none;
		-moz-box-shadow: none;
		box-shadow: none;
	}
	form input[type=submit] {
		font-family: "Raleway", Arial, Helvetica, sans-serif;
		background: #2d2c2a;
		font-size: 14px;
		font-weight: 600;
		padding: 15px 20px;
		-webkit-border-radius: 3px;
		-moz-border-radius: 3px;
		border-radius: 3px;
		-webkit-box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
	}
	form input[type=submit]:hover { background: #454441 !important }
	
	strong { font-weight: 600 }
	.noneTextTransform { text-transform: none !important }
	
	/** Headers **/
	h1, h2, h3, h4 {
		margin: 0;
		padding: 0;
		background: none;
		line-height: normal;
		color: #2d2c2a;
	}
	h1 a, h2 a, h3 a, h4 a {
		color: #2d2c2a;
	}
	h1 {
		font-family: "Vidaloka", Arial, Helvetica, sans-serif;
		margin-bottom: 20px;
		font-size: 30px;
		text-transform: uppercase;
		cursor: default;
		border-bottom: 3px solid #d4c6aa;
		display: inline-table;
	}
	h3 {
		font-family: "Vidaloka", Arial, Helvetica, sans-serif;
		margin-bottom: 20px;
		font-size: 20px;
		text-transform: uppercase;
	}
	
	/** Paddings **/
	.padding30 { padding: 30px !important }
	
	ul.default li {
		list-style: inside square;
		margin-bottom: 10px;
	}
	
	ul.big li {
		font-size: 16px;
		margin-bottom: 2px;
	}
	
	.addthis {
		border-top: 1px solid #ccc;
		margin-top: 15px;
		padding-top: 10px;
	}
	.addthis h3 { margin-bottom: 5px !important }
	
	
	.boxShadow {
		-webkit-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
	}
	
	.borderRadiusLeft {
		overflow: hidden;
		-webkit-border-radius: 20px 0 0 0;
		-moz-border-radius: 20px 0 0 0;
		border-radius: 20px 0 0 0;
	}
	
	.noBorderRadius {
		-webkit-border-radius: 0 !important;
		-moz-border-radius: 0 !important;
		border-radius: 0 !important;
	}
	
	label {
		color: #555;
		font-size: 13px;
		font-weight: 700;
	}
	
	body {
		background: #e7e7e7 url(../img/bg-body.jpg) repeat;
		font-family: "Raleway", Arial, Helvetica, sans-serif;
		font-size: 13px;
		font-weight: 300;
		color: #333;
	}
	
	img {
		-webkit-border-radius: 3px;
		-moz-border-radius: 3px;
		border-radius: 3px;
	}
	
	a:hover img {
		filter: alpha(opacity=85);
		opacity: 0.85;
	}
	
	a { color: #2d2c2a }
	
	/** Bandeiras **/	
	.idiomasIcones {
		position: absolute;
		right: 0;
		top: 10px;
	}
	.idiomasIcones img {
		width: 23px;
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
		filter: alpha(opacity=40);
		opacity: 0.4;
	}
	.idiomasIcones li {
		float: left;
		margin-left: 10px;
	}
	
	/** Menu **/
	.menuMobile { display: none }
	.menuCont {
		position: fixed;
		width: 100%;
		background: rgba(255,255,255,0.95);
		z-index: 999;
		-webkit-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
	}
	.menu {
		font-size: 15px;
		letter-spacing: -0.5px;
		font-weight: 400;
		text-align: center;
		cursor: default;
	}
	.menu li {
		display: inline-block;
	}
	.menu a {
		display: inline-block;
		text-decoration: none;
		color: #2d2c2a;
		padding: 0 15px;
		height: 80px;
		line-height: 80px;
		border-bottom: 5px solid rgba(0,0,0,0);
	}
	.menu a:hover {
		color: #bdae8e;
		border-bottom: 5px solid #eadcbf;
	}
	
	/** Logo Cont **/
	.logoCont {
		background: #eadcbf;
		text-align: center;
		padding: 105px 0 20px 0;
		margin-bottom: 40px;
		-webkit-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
	}
	.logoCont img.logo {
		width: 480px;
	}
	
	/** Rodapé **/
	.rodapeCont {
		margin-top: 40px;
		padding: 30px 0;
		border-top: 1px solid #fff;
		-webkit-box-shadow: 0 -1px 0 0px rgba(0, 0, 0, 0.1);
		-moz-box-shadow: 0 -1px 0 0px rgba(0, 0, 0, 0.1);
		box-shadow: 0 -1px 0 0px rgba(0, 0, 0, 0.1);
	}
	img.gecemLogo {
		width: 91px;
	}
	.RianDesign {
		text-align: right;
	}
	.RianDesign img {
		width: 32px;
	}
	
	
	
	/** Paginação **/
	.paginacao2 {
		cursor: default;
		margin: 20px 0 40px 0;
		text-align: center;
		color: rgba(0,0,0,0.3);
	}
	.paginacao2 a,
	.paginacao2 .desabilitado,
	.paginacao2 .current {
		padding: 6px 10px;
		text-decoration: none;
		margin-right: 4px;
		-webkit-border-radius: 2px;
		-moz-border-radius: 2px;
		border-radius: 2px;
		border: 1px solid rgba(0,0,0,0);
	}
	.paginacao2 a:hover {
		border: 1px solid #fff;
	}
	.paginacao2 .current {
		color: #fff;
		background: #2d2c2a;
	}
	.paginacao2 .desabilitado {
		color: #999;
	}
	a:hover .nome { color: #444 }
	
	/** Lista genérica (ex: Agenda e Galeria de fotos) **/
	ul.lista1 li {
		line-height: 22px;
		font-size: 13px;
		margin: 0 10px 20px 10px;
		color: #999;
		background: #fff;
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
	}
	ul.lista1 h3 {
		font-size: 20px;
		margin-bottom: 10px;
		text-transform: none;
	}
	ul.lista1 h3 a { color: #2d2c2a !important }
	ul.lista1 h3 a:hover {
		color: #fff;
	}
	ul.lista1 li a { color: #aaa }
	ul.lista1 li .foto {
		margin-left: -5px;
	}
	ul.lista1 li .foto img {	
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
	}
	ul.lista1 li .cont { padding-top: 20px }
	ul.lista1 li .descricao-tablet { display: none }
	
	
	/** Lista genérica (ex: Agenda e Galeria de fotos) **/
	ul.lista2 li.NoLink,
	ul.lista2 a {
		display: block;
		padding: 20px 20px 10px 20px;
		background: #fff;
		margin-bottom: 10px;
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
	}
	ul.lista2 li.download {
		background-image: url(../img/icone-download@2x.png);
		background-position: 10px 15px;
		background-repeat: no-repeat;
		background-size: 30px 30px;
		padding-left: 50px;
	}
	ul.lista2 h3 {
		font-size: 18px;
		margin-bottom: 10px;
		text-transform: none;
	}
	ul.lista2 .imagemPub {
		width: 100px;
		float: left;
		margin: 0 20px 10px 0;
	}
	ul.lista2 .imagemPub img {
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
	}
	
	
	/** Lista Vídeos **/
	.listaVideos .item {
		margin-bottom: 20px;
	}
	.listaVideos .imagem {
		margin-bottom: 15px;
	}
	.listaVideos .imagem img {
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
	}
	.listaVideos h3 {
		text-transform: none;
		font-size: 18px;
		margin-bottom: 10px;
	}
	
	
	/** Notícia - Imagem interna **/
	.imagemTexto img {
		width: 50%;
		float: left;
		margin: 0 20px 10px 0;
	}
	
	/* Conteúdo lateral da página interna */
	.contLateral {
		background: #fff;
		padding: 20px;
		color: #888;
		border-bottom: 3px solid #ffcc2a;
	}
	.contLateral a { color: #888 }
	.contLateral .tit2 {
		font-size: 18px;
		letter-spacing: -0.5px;
		line-height: 20px;
		margin-bottom: 20px;
		font-weight: 600;
		text-transform: uppercase;
	}
	.contLateral hr {
		border-top: 1px solid #f3df9a;
	}
	
	
	/** Home **/
	.contHomeMosaico {
		background: #fff url(../img/bg-cont-branco.png) top right repeat-y;
		overflow: hidden;
		margin-bottom: 50px;
	}
	.contHomeMosaico h1 {
		font-family: "Vidaloka", Arial, Helvetica, sans-serif;
		color: #2d2c2a;
		font-size: 28px;
		text-transform: uppercase;
		margin-bottom: 20px;
	}
	.contHomeMosaico .texto {
		padding: 80px 70px 0 50px;
		color: #777;
		line-height: 26px;
		font-size: 14px;
	}
	
	
	/** Conts **/
	.maisItens {
		border-top: 1px solid #cdcdcd;
		text-align: right;
		padding-top: 5px;
		margin-top: 15px;
	}
	.cont50Sombra {
		background: url(../img/cont-50-sombra.png) top center no-repeat;
		background-size: 100% 20px;
		height: 20px;
	}
	
	/* Cont Bege */
	.contBege {
		background: #eadcbf url(../img/bg-cont-bege.png) top left repeat-y;
		padding: 20px 60px;
	}
	.contBege .maisItens { border-top: 1px solid #dccdaf }
	
	/* Cont Branco */
	.contBranco {
		background: #fff url(../img/bg-cont-branco.png) top right repeat-y;
		padding: 20px 60px;
	}
	
	/* Cont Eventos (home) */
	.contEventos {
		padding: 0;
	}
	.contEventos .cont {
		padding: 20px 50px 20px 0;
	}
	
	
	
	/** Chamada publicações (sobre rodapé) **/
	.chamadaPublicacoes { margin-top: 20px }
	.chamadaPublicacoes .item {
		min-height: 300px;
		background: #fff;
		padding: 30px 20px 20px 20px;
		text-align: center;
		color: #727272;
		-webkit-box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 3px 3px 0px rgba(0, 0, 0, 0.2);
	}
	.chamadaPublicacoes a {
		color: #444;
		text-decoration: none;
		display: block;
	}
	.chamadaPublicacoes a:hover .item { background: #f9f9f9 }
	.chamadaPublicacoes .icone img { width: 48px }
	.chamadaPublicacoes .icone { margin-bottom: 20px }
	.chamadaPublicacoes h1 {
		font-size: 14px;		
		font-family: "Vidaloka", Arial, Helvetica, sans-serif;
		text-transform: uppercase;
		margin-bottom: 10px;
		cursor: pointer;
	}
	
	.listaFotos .item { margin-bottom: 20px }
	
	/** iFrame Vídeo **/
	.videoPlayer { margin-bottom: 20px }
	.videoPlayer iframe {
		width: 100%;
		height: 428px;
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
	}
	
	
	/** Lista Veja Mais **/
	.listaVejaMais li {
		padding-bottom: 15px;
		margin-bottom: 15px;
		border-bottom: 1px solid #eee;
	}
	.listaVejaMais li:last-child {
		padding-bottom: 0;
		margin-bottom: 0;
		border-bottom: none;
	}
	.listaVejaMais .imagem {
		width: 40%;
		float: left;
		margin-right: 5%;
	}
	.listaVejaMais .titulo {
		font-weight: bold;
		line-height: 20px;
	}
	.listaVejaMais .descricao {
		width: 55%;
		float: left;
	}
	
	
	/** Mais vídeos **/
	.maisLinks li {
		margin-bottom: 20px;
	}
	.maisLinks .imagem {
		width: 40%;
		float: left;
		margin-right: 5%;
	}
	.maisLinks .titulo {
		font-size: 14px;
		font-weight: bold;
		line-height: 16px;
	}
	.maisLinks .titulo a { color: #666 }
	.maisLinks .descricao {
		width: 55%;
		float: left;
	}
	
}

@media screen and (min-width: 980px) {
	
	/** Com transition **/
	a,
	a img,
	.menu a,
	form input,
	select,
	textarea,
	.chamadaPublicacoes a .item {
		-webkit-transition: all 0.2s linear;
		-moz-transition: all 0.2s linear;
		-o-transition: all 0.2s linear;
		transition: all 0.2s linear;
	}
	
}


@media screen and (min-width: 980px) and (max-width: 1365px) {
	
	/** Home **/
	.contHomeMosaico .texto { padding: 30px 50px 0 20px }
	
}


@media screen and (min-width: 481px) and (max-width: 979px) {
	
	/** Home **/
	.contHomeMosaico h1 {
		font-size: 20px;
		margin-bottom: 15px;
	}
	.contHomeMosaico .texto {
		padding: 20px 20px 0 0;
		color: #777;
		line-height: 22px;
		font-size: 13px;
	}
	
	/* Cont Eventos (home) */
	.contEventos .foto {
		padding: 0;
	}
	
	/** Bandeiras **/
	.idiomasIcones { top: 5px }
	.idiomasIcones img { width: 20px }
	.idiomasIcones li { margin-left: 10px; }
	
	/** Menu **/
	.menu {
		font-size: 12px;
		letter-spacing: -0.2px;
	}
	.menu a {
		padding: 0 8px;
		height: 70px;
		line-height: 70px;
	}
	
	/** Logo Cont **/
	.logoCont { padding: 95px 0 20px 0 }
	
	/** Rodapé **/
	.rodapeCont { padding: 20px 0 }
	
	/** Lista genérica (ex: Agenda e Galeria de fotos) **/
	ul.lista1 li .descricao-tablet { display: block }
	ul.lista1 li .descricao-desktop { display: none }
	
}

@media screen and (max-width: 767px) {
	
	/** Headers **/
	h1 { font-size: 24px }
	h3 { font-size: 18px}
	
	/** Paddings **/
	.padding30 { padding: 20px !important }
	
	/** Bandeiras **/	
	.idiomasIcones {
		position: inherit;
		right: auto;
		top: auto;
		text-align: center;
		margin-bottom: 5px;
	}
	.idiomasIcones img {
		width: 23px;
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
		filter: alpha(opacity=40);
		opacity: 0.4;
	}
	.idiomasIcones li {
		float: none;
		display: inline-block;
		margin-left: 5px;
	}
	
	/** Logo Cont **/
	.logoCont {
		background: #eadcbf;
		text-align: center;
		padding: 110px 0 20px 0;
		margin-bottom: 30px;
		-webkit-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 4px 5px 0px rgba(0, 0, 0, 0.2);
	}
	
	/** Logo **/
	.logoCont img.logo { width: 80% }
	
	/** Menu **/
	.menuCont {
		padding: 10px 0 0 0;		
	}
	.menu { display: none }
	.menuMobile {
		display: block;
		width: 80%;
		margin: 0 auto 5px;
	}
	.menuMobile select {
		color: #000;
		border: 1px solid #aaa;
		background: #fff;		
		padding: 5px;
	}
	.menuMobile select option {
		padding-bottom: 4px;
	}
	
	/** Lista genérica (ex: Agenda e Galeria de fotos) **/
	ul.lista1 li .foto { margin-bottom: 10px }
	
	/** Lista 2 (ex: vídeos) **/
	.lista2 .titulo { font-size: 13px }
	ul.lista2 .imagemPub {
		margin-right: 10px;
		width: 60px;
	}
	
	/** Notícia - Imagem interna **/
	.imagemTexto img {
		width: 100%;
		float: left;
		margin: 0 0 15px 0;
	}
	
	/** Rodapé **/
	.rodapeCont {
		margin-top: 30px;
		padding: 20px 0;
	}
	
	
	/** Chamada publicações (sobre rodapé) **/
	.chamadaPublicacoes .item { margin-bottom: 20px }
	
	
	/** Lista genérica (ex: Agenda e Galeria de fotos) **/
	ul.lista1 li {
		line-height: 22px;
		font-size: 13px;
		margin: 0 10px 20px 10px;
		color: #999;
		background: #fff;
		-webkit-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		-moz-box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
		box-shadow: 0 2px 2px 0px rgba(0, 0, 0, 0.2);
	}
	ul.lista1 h3 {
		font-size: 20px;
		margin-bottom: 10px;
		text-transform: none;
	}
	ul.lista1 h3 a { color: #2d2c2a !important }
	ul.lista1 h3 a:hover {
		color: #fff;
	}
	ul.lista1 li a { color: #aaa }
	ul.lista1 li .foto {
		margin: 0 -15px 10px -5px;
	}
	ul.lista1 li .foto img {	
		-webkit-border-radius: 0;
		-moz-border-radius: 0;
		border-radius: 0;
	}
	ul.lista1 li .cont { padding: 5px 20px 20px 20px; }
	ul.lista1 li .descricao-tablet { display: block !important }
	
	/** iFrame Vídeo **/
	.videoPlayer iframe { height: 200px }
	
}

@media screen and (max-width: 480px) {
	
}

@media screen and (max-width: 320px) {
	
	/** Logo **/
	.logoCont img.logo { width: 100% }
	
		
}